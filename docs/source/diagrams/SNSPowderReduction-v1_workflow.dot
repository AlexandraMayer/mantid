digraph SNSPowderReduction {
  label="SNSPowderReduction Flowchart"
  $global_style

  subgraph params {
    $param_style
    RunNumber
    Sum
    TimeFilterWall
    CalibrationFile
  }

  subgraph algorithms {
    $algorithm_style
    focusAndSum [label="FocusAndSum v1"]
  }


  subgraph decisions{
    $decision_style
    haveMPI [label="Have MPI?"]
    isSum [label="Is Sum True?"]
    isLastRunNumber [label="Is Last Run Number?"]
  }
  

  subgraph values {
    $value_style
    SamRun [label="Processed\nSample"]
  }

  Sum		    -> isSum

  isSum		    -> focusAndSum  [label="Yes"]
  RunNumber	    -> focusAndSum
  TimeFilterWall    -> focusAndSum
  CalibrationFile   -> focusAndSum
  focusAndSum	    -> SamRun

  isSum		    -> isLastRunNumber [label="No"]


}
