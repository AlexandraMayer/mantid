<ServerManagerConfiguration>
  <!-- Begin EventNexusReader -->
  <ProxyGroup name="sources">
    <SourceProxy name="EventNexusReader" class="vtkEventNexusReader">
	    <ProxyProperty name="ClipFunction" command="SetClipFunction" label="Rebinning Cut Type">
           <ProxyGroupDomain name="groups">
               <Group name="implicit_functions"/>
           </ProxyGroupDomain>
           <ProxyListDomain name="proxy_list">
              <Proxy group="implicit_functions" name="Plane" />
           </ProxyListDomain>
           <Documentation>
             This property specifies the parameters of the clip function (an implicit plane) used to clip/rebin the dataset.
           </Documentation>
        </ProxyProperty>
		<IntVectorProperty
         name="Apply Clipping"
         command="SetApplyClip"
         number_of_elements="1"
         default_values="1">
         <BooleanDomain name="bool"/>
        </IntVectorProperty>
		<DoubleVectorProperty
         name="Plane Width"
         command="SetWidth"
         number_of_elements="1"
	     default_values="1">
	    </DoubleVectorProperty>
      	<IntVectorProperty
         name="X Bins"
         command="SetXBins"
         number_of_elements="1"
         default_values="50">
        </IntVectorProperty>
	    <IntVectorProperty
         name="Y Bins"
         command="SetYBins"
         number_of_elements="1"
         default_values="50">
        </IntVectorProperty>
	    <IntVectorProperty
         name="Z Bins"
         command="SetZBins"
         number_of_elements="1"
         default_values="50">
        </IntVectorProperty>
        <DoubleVectorProperty
         name="Min Signal"
         command="SetMinThreshold"
         number_of_elements="1"
	     default_values="0.1"
	    >
	   </DoubleVectorProperty>
	   <DoubleVectorProperty
         name="Max Signal"
         command="SetMaxThreshold"
         number_of_elements="1"
	     default_values="10000">
	   </DoubleVectorProperty>
       <StringVectorProperty
        name="FileName"
        command="SetFileName"
        number_of_elements="1">
        <FileListDomain name="files"/>
      </StringVectorProperty>
    </SourceProxy>
  </ProxyGroup>
  <!-- End EventNexusReader -->
</ServerManagerConfiguration>