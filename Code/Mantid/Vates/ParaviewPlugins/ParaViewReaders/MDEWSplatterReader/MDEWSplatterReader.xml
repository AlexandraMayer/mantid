<ServerManagerConfiguration>
  <ProxyGroup name="sources">
    <SourceProxy name="MDEWSplatterReader" class="vtkMDEWSplatterReader">
	   <IntVectorProperty
         name="Load All Into Memory"
         command="SetInMemory"
         number_of_elements="1"
         default_values="0">
         <BooleanDomain name="bool"/>
       </IntVectorProperty>
	   <IntVectorProperty
         name="Number of Points"
         command="SetNumberOfPoints"
         number_of_elements="1"
         default_values="10000">
       </IntVectorProperty>
       <StringVectorProperty
        name="FileName"
        command="SetFileName"
        number_of_elements="1">
        <FileListDomain name="files"/>
      </StringVectorProperty>
    </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>